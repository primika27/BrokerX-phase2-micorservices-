  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Load Balancing Performance Overview",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Requests Per Second by Instance Count",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "showPoints": "never",
            "spanNulls": true
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 100},
              {"color": "red", "value": 200}
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=~\".*\"}[5m])) by (instance_count)",
          "legendFormat": "{{instance_count}} instances",
          "refId": "A"
        },
        {
          "expr": "sum(rate(nginx_http_requests_total[5m])) by (instance_count)",
          "legendFormat": "NGINX - {{instance_count}} instances",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Response Time by Instance Count (P95)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 1
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "none"
          },
          "unit": "ms",
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 500},
              {"color": "red", "value": 1000}
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[5m])) by (le, instance_count)) * 1000",
          "legendFormat": "{{instance_count}} instances - P95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_server_requests_seconds_bucket[5m])) by (le, instance_count)) * 1000",
          "legendFormat": "{{instance_count}} instances - P50",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Error Rate Comparison",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 9
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2
          },
          "unit": "percent",
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 10}
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[5m])) by (instance_count) / sum(rate(http_server_requests_seconds_count[5m])) by (instance_count) * 100",
          "legendFormat": "{{instance_count}} instances - Error Rate",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Throughput Saturation Analysis",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 9
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10
          },
          "unit": "percent"
        }
      },
      "targets": [
        {
          "expr": "avg(rate(container_cpu_usage_seconds_total{name=~\".*service.*\"}[5m])) by (instance_count) * 100",
          "legendFormat": "{{instance_count}} instances - CPU Usage",
          "refId": "A"
        },
        {
          "expr": "avg(container_memory_usage_bytes{name=~\".*service.*\"}) by (instance_count) / avg(container_spec_memory_limit_bytes{name=~\".*service.*\"}) by (instance_count) * 100",
          "legendFormat": "{{instance_count}} instances - Memory Usage",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Caching Analysis",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 17
      },
      "collapsed": false
    },
    {
      "title": "Cache Hit Rate",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 18
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 80}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "targets": [
        {
          "expr": "redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total) * 100",
          "legendFormat": "Cache Hit Rate",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Response Time: With vs Without Cache",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 6,
        "w": 18,
        "x": 6,
        "y": 18
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2
          },
          "unit": "ms"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket{uri=~\"/api/(stores|reports|quotes).*\", cache_enabled=\"true\"}[5m])) * 1000",
          "legendFormat": "With Cache - {{uri}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket{uri=~\"/api/(stores|reports|quotes).*\", cache_enabled=\"false\"}[5m])) * 1000",
          "legendFormat": "Without Cache - {{uri}}",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Load Balancing Stats",
      "type": "table",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "displayMode": "table"
          }
        }
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count[5m])) by (instance_count, instance)",
          "format": "table",
          "legendFormat": "{{instance_count}} instances",
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {},
            "indexByName": {},
            "renameByName": {
              "instance_count": "Instance Count",
              "instance": "Instance",
              "Value": "RPS"
            }
          }
        }
      ]
    },
    {
      "title": "Fault Tolerance Metrics",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2
          },
          "unit": "short"
        }
      },
      "targets": [
        {
          "expr": "up{job=~\".*service.*\"}",
          "legendFormat": "{{job}} - {{instance}}",
          "refId": "A"
        }
      ]
    }
  ],
